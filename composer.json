    "description": "A basic PSR-7 and PSR-17 compatible stream utility library",
    "license": "MIT",
    "homepage": "http://docs.talesoft.codes/php/tale/stream",
    "authors": [
        {
            "name": "Torben KÃ¶hn",
            "email": "torben@talesoft.codes"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "autoload": {
        "psr-4": {
